{
	"name": "rcloner",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"analyze": "cross-env ANALYZE=true NEXT_TELEMETRY_DISABLED=1 next build",
		"build": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
		"dev": "cross-env NEXT_TELEMETRY_DISABLED=1 next dev",
		"docker:build": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose build",
		"docker:dev": "docker-compose up",
		"format": "prettier --write \"**/**.{js,jsx,ts,tsx,json,css,scss,html,yml}\"",
		"lint": "eslint . --ext .ts,.tsx --ignore-path .eslintignore",
		"prepare": "husky install",
		"start": "cross-env NODE_ENV=production next start"
	},
	"dependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@monaco-editor/react": "^4.5.1",
		"@mui/material": "^5.14.5",
		"@next/bundle-analyzer": "^13.4.19",
		"@types/lodash": "^4.14.197",
		"@types/node": "20.5.1",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"client-only": "^0.0.1",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"monaco-editor": "^0.41.0",
		"next": "13.4.19",
		"notistack": "^3.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-focus-lock": "^2.9.5",
		"server-only": "^0.0.1",
		"sharp": "^0.32.5",
		"type-fest": "^4.2.0",
		"typescript": "5.1.6",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@babel/core": "^7.22.10",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"cross-env": "^7.0.3",
		"eslint": "8.47.0",
		"eslint-config-next": "13.4.19",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"sass": "^1.66.1"
	}
}
