{
	"name": "rcloner",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"analyze": "cross-env ANALYZE=true NEXT_TELEMETRY_DISABLED=1 next build",
		"build": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
		"dev": "cross-env NEXT_TELEMETRY_DISABLED=1 next dev",
		"format": "prettier --write \"**/**.{js,jsx,ts,tsx,json,css,scss,html,yml}\"",
		"lint": "eslint . --ext .ts,.tsx --ignore-path .eslintignore",
		"prepare": "husky install",
		"start": "cross-env NODE_ENV=production next start"
	},
	"dependencies": {
		"@types/node": "20.5.1",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"next": "13.4.19",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "5.1.6"
	},
	"devDependencies": {
		"@babel/core": "^7.22.10",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"cross-env": "^7.0.3",
		"eslint": "8.47.0",
		"eslint-config-next": "13.4.19",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"husky": "^8.0.3",
		"prettier": "^3.0.2",
		"pretty-quick": "^3.1.3",
		"sass": "^1.66.1"
	}
}
